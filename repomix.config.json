{
  "output": {
    "filePath": "output.txt",
    "style": "plain",
    "parsableStyle": false,
    "fileSummary": true,
    "directoryStructure": true,
    "removeComments": true,
    "removeEmptyLines": true,
    "compress": false,
    "topFilesLength": 5,
    "showLineNumbers": false,
    "copyToClipboard": false
  },
  "include": [],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "priv/",
      "rel/",
      "repomix.config.json",
      ".github/",
      "assets/",
      "test/",
      ".repomixignore",
      ".pre-commit-config.yaml",
      "fly.toml",
      ".dockerignore",
      "Dockerfile",
      "README.md"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}
